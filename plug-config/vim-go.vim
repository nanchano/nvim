"let g:go_highlight_functions = 1
"let g:go_highlight_function_calls = 1
"Indented to 4
set shiftwidth=4
set softtabstop=4
"Tab key width
set tabstop=4"
set autoindent
set cindent
"The width of the Tab key
set tabstop=4
"Unified indentation is 4
set softtabstop=4
set shiftwidth=4
"Use spaces instead of tabs
set expandtab
"Use tabs at the beginning of the line and paragraph
set smarttab
"Folding
let g:go_fold_enable = ['block', 'import', 'varconst', 'package_comment']
